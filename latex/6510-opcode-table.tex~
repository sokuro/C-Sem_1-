0xD6  &  DEC  &  0x2  &  0x6  &  zpx  &  decrement  &  {adr}:={adr}-1  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----*- }} & \texttt{    M = (M - 1) & \$FF \n    P.N = M.7 \n    P.Z = (M==0) ? 1:0  \n \n \n    P.N = B.7                  \n \n    ELSE \n      P.C = (t>255) ? 1:0 \n    A = t & 0xFF                 \n \n} &  \textbf{\texttt{ DEC  \$AB,X}} \\
0xD8  &  CLD  &  0x1  &  0x2  &  imp  &  clear decimal  &  D:=0  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ ---0--- }} & \texttt{    P.D = 0  \n \n    P.Z = (M==0) ? 1:0  \n \n \n    P.N = B.7                  \n \n    ELSE \n      P.C = (t>255) ? 1:0 \n    A = t & 0xFF                 \n \n} &  \textbf{\texttt{ CLD  }} \\
0xD9  &  CMP  &  0x3  &  0x4  &  aby  &  compare (with accumulator)  &  A-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----** }} & \texttt{    t = A - M \n    P.N = t.7 \n    P.C = (A>=M) ? 1:0 \n    P.Z = (t==0) ? 1:0  \n \n    P.N = B.7                  \n \n    ELSE \n      P.C = (t>255) ? 1:0 \n    A = t & 0xFF                 \n \n} &  \textbf{\texttt{ CMP  \$ABCD,Y}} \\
0xDD  &  CMP  &  0x3  &  0x4  &  abx  &  compare (with accumulator)  &  A-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----** }} & \texttt{    t = A - M \n    P.N = t.7 \n    P.C = (A>=M) ? 1:0 \n    P.Z = (t==0) ? 1:0  \n \n    P.N = B.7                  \n \n    ELSE \n      P.C = (t>255) ? 1:0 \n    A = t & 0xFF                 \n \n} &  \textbf{\texttt{ CMP  \$ABCD,X}} \\
0xDE  &  DEC  &  0x3  &  0x7  &  abx  &  decrement  &  {adr}:={adr}-1  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----*- }} & \texttt{    M = (M - 1) & \$FF \n    P.N = M.7 \n    P.Z = (M==0) ? 1:0  \n \n \n    P.N = B.7                  \n \n    ELSE \n      P.C = (t>255) ? 1:0 \n    A = t & 0xFF                 \n \n} &  \textbf{\texttt{ DEC  \$ABCD,X}} \\
0xE0  &  CPX  &  0x2  &  0x2  &  imm  &  compare with X  &  X-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----** }} & \texttt{    t = X - M \n    P.N = t.7 \n    P.C = (X>=M) ? 1:0 \n    P.Z = (t==0) ? 1:0  \n \n    P.N = B.7                  \n \n    ELSE \n      P.C = (t>255) ? 1:0 \n    A = t & 0xFF                 \n \n} &  \textbf{\texttt{ CPX  #\$AB    }} \\
0xE1  &  SBC  &  0x2  &  0x6  &  izx  &  subtract with carry  &  A:=A-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ **---** }} & \texttt{    IF (P.D) \n      t = bcd(A) - bcd(M) - !P.C \n      P.V = (t>99 OR t<0) ? 1:0 \n    ELSE \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ SBC  (\$A5,X)}} \\
0xE4  &  CPX  &  0x2  &  0x3  &  zp  &  compare with X  &  X-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----** }} & \texttt{    t = X - M \n    P.N = t.7 \n    P.C = (X>=M) ? 1:0 \n    P.Z = (t==0) ? 1:0  \n \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ CPX  (\$A5,X)}} \\
0xE5  &  SBC  &  0x2  &  0x3  &  zp  &  subtract with carry  &  A:=A-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ **---** }} & \texttt{    IF (P.D) \n      t = bcd(A) - bcd(M) - !P.C \n      P.V = (t>99 OR t<0) ? 1:0 \n    ELSE \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ SBC  (\$A5,X)}} \\
0xE6  &  INC  &  0x2  &  0x5  &  zp  &  increment  &  {adr}:={adr}+1  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----*- }} & \texttt{    M = (M + 1) & \$FF \n    P.N = M.7 \n    P.Z = (M==0) ? 1:0  \n \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ INC  (\$A5,X)}} \\
0xE8  &  INX  &  0x1  &  0x2  &  imp  &  increment X  &  X:=X+1  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----*- }} & \texttt{    X = X + 1 \n    P.Z = (X==0) ? 1:0 \n    P.N = X.7           \n \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ INX  }} \\
0xE9  &  SBC  &  0x2  &  0x2  &  imm  &  subtract with carry  &  A:=A-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ **---** }} & \texttt{    IF (P.D) \n      t = bcd(A) - bcd(M) - !P.C \n      P.V = (t>99 OR t<0) ? 1:0 \n    ELSE \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ SBC  #\$AB    }} \\
0xEA  &  NOP  &  0x1  &  0x2  &  imp  &  no operation  &     & \mbox{\texttt{NVBDIZC}}\newline\texttt{ ------- }} & \texttt{    ~none~  \n \n      P.V = (t>99 OR t<0) ? 1:0 \n    ELSE \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ NOP  }} \\
0xEC  &  CPX  &  0x3  &  0x4  &  abs  &  compare with X  &  X-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----** }} & \texttt{    t = X - M \n    P.N = t.7 \n    P.C = (X>=M) ? 1:0 \n    P.Z = (t==0) ? 1:0  \n \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ CPX  \$ABCD}} \\
0xED  &  SBC  &  0x3  &  0x4  &  abs  &  subtract with carry  &  A:=A-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ **---** }} & \texttt{    IF (P.D) \n      t = bcd(A) - bcd(M) - !P.C \n      P.V = (t>99 OR t<0) ? 1:0 \n    ELSE \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ SBC  \$ABCD}} \\
0xEE  &  INC  &  0x3  &  0x6  &  abs  &  increment  &  {adr}:={adr}+1  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----*- }} & \texttt{    M = (M + 1) & \$FF \n    P.N = M.7 \n    P.Z = (M==0) ? 1:0  \n \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ INC  \$ABCD}} \\
0xF0  &  BEQ  &  0x2  &  0x2  &  rel  &  branch on equal (zero set)  &  branch on Z=1  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ ------- }} & \texttt{    if (P.Z == 1) GOTO (PC+M)  \n \n    P.Z = (M==0) ? 1:0  \n \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ BEQ  \$AB}} \\
0xF1  &  SBC  &  0x2  &  0x5  &  izy  &  subtract with carry  &  A:=A-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ **---** }} & \texttt{    IF (P.D) \n      t = bcd(A) - bcd(M) - !P.C \n      P.V = (t>99 OR t<0) ? 1:0 \n    ELSE \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ SBC  (\$A5),X}} \\
0xF5  &  SBC  &  0x2  &  0x4  &  zpx  &  subtract with carry  &  A:=A-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ **---** }} & \texttt{    IF (P.D) \n      t = bcd(A) - bcd(M) - !P.C \n      P.V = (t>99 OR t<0) ? 1:0 \n    ELSE \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ SBC  \$AB,X}} \\
0xF6  &  INC  &  0x2  &  0x6  &  zpx  &  increment  &  {adr}:={adr}+1  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----*- }} & \texttt{    M = (M + 1) & \$FF \n    P.N = M.7 \n    P.Z = (M==0) ? 1:0  \n \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ INC  \$AB,X}} \\
0xF8  &  SED  &  0x1  &  0x2  &  imp  &  set decimal  &  D:=1  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ ---1--- }} & \texttt{    P.D = 1  \n \n    P.Z = (M==0) ? 1:0  \n \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ SED  }} \\
0xF9  &  SBC  &  0x3  &  0x4  &  aby  &  subtract with carry  &  A:=A-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ **---** }} & \texttt{    IF (P.D) \n      t = bcd(A) - bcd(M) - !P.C \n      P.V = (t>99 OR t<0) ? 1:0 \n    ELSE \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ SBC  \$ABCD,Y}} \\
0xFD  &  SBC  &  0x3  &  0x4  &  abx  &  subtract with carry  &  A:=A-{adr}  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ **---** }} & \texttt{    IF (P.D) \n      t = bcd(A) - bcd(M) - !P.C \n      P.V = (t>99 OR t<0) ? 1:0 \n    ELSE \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ SBC  \$ABCD,X}} \\
0xFE  &  INC  &  0x3  &  0x7  &  abx  &  increment  &  {adr}:={adr}+1  & \mbox{\texttt{NVBDIZC}}\newline\texttt{ *----*- }} & \texttt{    M = (M + 1) & \$FF \n    P.N = M.7 \n    P.Z = (M==0) ? 1:0  \n \n      t = A - M - !P.C \n      P.V = (t>127 OR t<-128) ? 1:0 \n    P.C = (t>=0) ? 1:0 \n    P.N = t.7 \n    P.Z = (t==0) ? 1:0 \n    A = t & 0xFF                     \n \n} &  \textbf{\texttt{ INC  \$ABCD,X}} \\

